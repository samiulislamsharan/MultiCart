    "securityDefinitions": {
        "APIKeyHeader": {
            "type": "apiKey",
            "in": "header",
            "name": "api-key"
        }
    },
    "definitions": {
        "user": {
            "type": "object",
            "properties": {
                "user_id": {
                    "type": "number"
                },
                "name": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                }
            }
        }
    },
    "schemes": [
        "http",
        "https"
    ]
